<main>
  <h1>Browser</h1>
  <form action="/" method="GET">
    <input type="text" class="search" name="search" placeholder="Search..." value="{{query.search}}">
    <input type="number" name="from" class="difficulty" placeholder="Difficulty from..." value="{{query.from}}">
    <span>-</span>
    <input type="number" name="to" class="difficulty" placeholder="Difficulty to..." value="{{query.to}}">
    <input type="submit" value="search">
  </form>
  {{#each cubes}}
    {{>cube}}
  {{else}}
  <h2>NO CUBES WERE FOUND</h2>
  {{/each}}

</main>